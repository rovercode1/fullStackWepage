<% include ../partials/header %>
<section id="search">
  <h1>Search</h1>
  <div class="container">
    <div id="searchBox">
        <div class="row">
          <%  product.forEach(function(product){%>
              <div class="col-lg-4 col-md-4 col-sm-6">
                <div class="product">
                  <div class="image">
                    <img  src="<%= product.image %>">
                  </div>
                  <div id="productDesc">
                    <p>
                      <a href="/shop/<%= product._id %>"><%=  product.title.substring(0,80) %>...</a>
                    </p>
                  </div>
                </div>
              </div>
          <% }) %>
        </div>
    </div>
  </div>
</section>
<% include ../partials/footer %>